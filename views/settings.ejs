   <body style="background-image: none">
      <div class="topbar">
         <div class="left-buttons">
            <a id="exit" onclick="exit()" style="display: none">
            <span style="font-weight: bold;">Exit</span>
            </a>
            <a id="homebtn" href="/" class="<%= path === '/' ? 'nav-active' : 'nav-btn' %>"><span>Home</span></a>
            <a href="/a" class="<%= path === '/a' ? 'nav-active' : 'nav-btn' %>"><span>Apps</span></a>
            <a href="/g" class="<%= path === '/g' ? 'nav-active' : 'nav-btn' %>"><span>Games</span></a>
         </div>
         <form id="form" class="center-form">
            <input autofocus autocomplete="off" class="search" id="search" placeholder="Search Equinox Settings"
               required />
            <button type="submit" class="submit-button">
            <i class="fas fa-search searchicon"></i>
            </button>
         </form>
         <div class="right-section">
            <a class="nav-btn" href="/s"
               style="display: flex; justify-content: flex-end; margin-left: auto;">Settings</a>
            <a id="time" class="nav-btn"
               style="display: flex; justify-content: flex-end; margin-left: auto;">00:00PM</a>
         </div>
      </div>
      <div class="games-body">
         <h3 class="category-heading" id="recent-txt"><img class="recent-img" src="/assets/eqsettings.png"
            style="padding-left: 10px"></h3>
         <article class="card">
            <article class="grid gap-big">
               <div class="title">
                  Search Settings
                  <p>Settings related to Equinox Search</p>
               </div>
               <div class="grid col-2">
                  <fieldset>
                     <label>Select a search engine</label>
                     <select id="engine" class="input-group" name="engine">
                        <option value="google">Google</option>
                        <option value="duckduckgo">DuckDuckGo</option>
                        <option value="yahoo">Yahoo</option>
                     </select>
                  </fieldset>
                  <fieldset>
                     <label>Select a proxy server</label>
                     <select id="proxy" class="input-group" name="proxy">
                        <option value="uv">Ultraviolet</option>
                        <option value="dy">Dynamic</option>
                     </select>
                  </fieldset>
               </div>
            </article>
         </article>
         <article class="card">
            <article class="grid gap-big">
               <div class="title">
                  Preferences & Security
                  <p>Customize your experience on Equinox</p>
               </div>
               <div class="grid col-2">
                  <fieldset>
                     <label>
                        Site Theme
                        <p>Change the theme across the entire site.</p>
                     </label>
                     <select id="themes" class="input-group" name="themes">
                        <option value="default">Default</option>
                        <option value="flashbang">Flashbang</option>
                        <option value="kongrats">Kong Rats</option>
                        <option value="ocean">Ocean</option>
                     </select>
                  </fieldset>
                  <fieldset>
                     <label>
                        Custom Background
                        <p>Change the background on the home page.</p>
                     </label>
                     <input type="text" id="custom-background" name="custom-background" placeholder="Enter URL of custom background image" />

                     <span class="error" id="error"></span>
                     <div class="button-group">
                        <button type="button" class="button" id="set-background">Save</button>
                        <button type="button" class="button" id="remove-background">Remove</button>
                     </div>
                  </fieldset>
               </div>
               <div class="grid col-2">
                  <fieldset>
                     <label>
                        Stealth Mode
                        <p>Coming soon...
                        </p>
                     </label>
   
                  </fieldset>
                  <fieldset>
                     <label>
                        Tab Cloaking
                        <p>Change the tab icon and title of the page.</p>
                     </label>
                     <div class="button-group">
                        <button type="button" id="google-cloak" onclick="cloakPage('google')" class="cloak-button"><img class="cloak-img"
                           src="/assets/img/cloak/google.webp"></button>
                        <button type="button" id="docs-cloak" onclick="cloakPage('docs')" class="cloak-button"><img class="cloak-img"
                           src="/assets/img/cloak/google-docs.webp"></button>
                        <button type="button" id="drive-cloak" onclick="cloakPage('drive')" class="cloak-button"><img class="cloak-img"
                           src="/assets/img/cloak/google-drive.webp"></button>
                        <button type="button" id="classroom-cloak" onclick="cloakPage('classroom')" class="cloak-button"><img class="cloak-img"
                           src="/assets/img/cloak/home.webp"></button>
                        <button type="button" id="chrome-cloak" onclick="cloakPage('chrome')" class="cloak-button"><img class="cloak-img"
                           src="/assets/img/cloak/chrome.webp"></button>


                     </div>
                     <div class="button-group">
                        <button type="button" class="button" id="stealth-off">Reset</button>
                     </div>
                  </fieldset>
               </div>
            </article>
         </article>
         <article class="card">
            <article class="grid gap-big">
               <div class="title">
                  Version Details
                  <p>Relevant system and build information of Equinox</p>
               </div>
               <div class="grid col-2">
                  <fieldset>
                     <label>
                        Version Number
                        <p id="eqversion">1.36<span id="latestorno"> (Latest)</span></p>
                     </label>
                     <label>
                        Build Number
                        <p id="buildnumber"></p>
                     </label>
                     <label>
